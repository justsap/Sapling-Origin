{
    "type": "origins:multiple",
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "function img:regenfield"
        },
        "interval": 5,
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_timer",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "healer": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:area_of_effect",
            "radius": 3.5,
            "shape": "cube",
            "bientity_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:apply_effect",
                            "effects": [
                                {
                                    "effect": "minecraft:regeneration",
                                    "duration": 160,
                                    "amplifier": 2
                                },
                                {
                                    "effect": "minecraft:saturation",
                                    "duration": 160,
                                    "amplifier": 0,
                                    "show_icon": false,
                                    "show_particles": false
                                },
                                {
                                    "effect": "minecraft:resistance",
                                    "duration": 80,
                                    "amplifier": 1,
                                    "show_icon": false,
                                    "show_particles": false
                                }
                            ]
                        }
                    },
                    {
                        "type": "origins:actor_action",
                        "action": {
                            "type": "origins:apply_effect",
                            "effects": [
                                {
                                    "effect": "minecraft:resistance",
                                    "duration": 160,
                                    "amplifier": 1
                                },
                                {
                                    "effect": "minecraft:saturation",
                                    "duration": 160,
                                    "amplifier": 0,
                                    "show_icon": false,
                                    "show_particles": false
                                }
                            ]
                        }
                    }
                ]
            },
            "bientity_condition": {
                "type":"origins:target_condition",
                "condition": {
                        "type": "origins:in_tag",
                        "tag": "sapcraft:regenfield-entities"
                }
            }
        },
        "interval": 10,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "apugli:entity_in_radius",
                    "condition": {
                        "type": "origins:in_tag",
                        "tag": "sapcraft:regenfield-entities"
                    },
                    "radius": 2.5,
                    "comparison": ">=",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_timer",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "timer": {
        "type": "origins:cooldown",
        "cooldown": 200,
        "comment": "//You can change the 'cooldown' value to whatever you want the TIMER to last for (remember, the cooldown is measured in ticks)",
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 9,
            "inverted": true,
            "comment": "//You can edit this 'hud_render' as you please."
        }
    },
    "cooldown": {
        "type": "origins:cooldown",
        "cooldown": 400,
        "comment": "//You can change the 'cooldown' value to whatever you want the COOLDOWN to last for (remember, the cooldown is measured in ticks)",
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/spiderkolo/resource_bar_02.png",
            "bar_index": 9,
            "comment": "//You can edit this 'hud_render' as you please."
        }
    },
    "reset": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_timer",
                    "change": 200,
                    "operation": "set"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "*:*_trigger",
                    "change": 604,
                    "operation": "set"
                }
            ]
        }
    },
    "trigger": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:trigger_cooldown",
                    "power": "*:*_timer"
                },
                {
                    "type": "origins:add_xp",
                    "levels": -1
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.mangrove_roots.place",
                    "category": "players",
                    "volume": 2.0
                },
                {
                    "type": "origins:play_sound",
                    "sound": "minecraft:block.beacon.power_select",
                    "category": "players",
                    "volume": 1.5
                }
            ]
        },
        "cooldown": 604,
        "comment": "//Make this equal the ticks of the subpowers `timer` and `cooldown` if they were added together. (So if timer is 200, and coolddown is 400, it should equal 600)",
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "apugli:entity_in_radius",
                    "condition": {
                        "type": "origins:in_tag",
                        "tag": "sapcraft:regenfield-entities"
                    },
                    "radius": 2.5,
                    "comparison": ">=",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "*:*_cooldown",
                    "compare_to": 0,
                    "comparison": "=="
                },
                {
                    "type": "origins:xp_levels",
                    "comparison": ">=",
                    "compare_to": 1
                }
            ]
        },
        "key": {
            "key": "key.origins.ternary_active"
        },
        "hud_render": {
            "should_render": false
        }
    },
    "cooldown_trigger": {
        "type": "origins:action_over_time",
        "falling_action": {
            "type": "origins:trigger_cooldown",
            "power": "*:*_cooldown"
        },
        "condition": {
            "type": "origins:resource",
            "resource": "*:*_timer",
            "comparison": ">",
            "compare_to": 0
        },
        "interval": 2
    },
    "name": "Share Nutrients",
    "description": "If in proximity with a player or any land animal, you can create a circle that heals those inside it, but it takes away §n1 XP level.",
    "badges": [
        {
            "type": "origins:keybind",
            "sprite": "origins:textures/gui/community/silent/badge/shapes/shape_square.png",
            "text": "Active ability, use with %s §o- Must have entity in a 4 block radius"
        }
    ]
}